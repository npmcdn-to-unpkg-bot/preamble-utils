{
  "name": "preamble-utils",
  "version": "2.0.1",
  "description": "Javascript utility library for Shopify themes",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/preamble.js && webpack -p src/index.js dist/preamble.min.js",
    "test": "mocha --compilers js:babel/register",
    "test:cov": "babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --recursive",
    "prepublish": "npm run test && npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sdn90/preamble-utils"
  },
  "author": "Steven Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sdn90/preamble-utils/issues"
  },
  "homepage": "https://github.com/sdn90/preamble-utils",
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^5.3.2",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.1",
    "expect": "^1.9.0",
    "exports-loader": "^0.6.2",
    "imports-loader": "^0.6.4",
    "isparta": "^3.0.3",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.0"
  },
  "dependencies": {
    "lodash": "^3.10.1"
  }
}
